{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/application/src",
  "monorepo": true,
  "root": "apps/application",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/application/tsconfig.app.json"
  },
  "projects": {
    "application": {
      "type": "application",
      "root": "apps/application",
      "entryFile": "main",
      "sourceRoot": "apps/application/src",
      "compilerOptions": {
        "tsConfigPath": "apps/application/tsconfig.app.json"
      }
    },
    "domain": {
      "type": "application",
      "root": "apps/domain",
      "sourceRoot": "apps/domain/src",
      "compilerOptions": {
        "tsConfigPath": "apps/domain/tsconfig.app.json"
      }
    },
    "infrastructure": {
      "type": "application",
      "root": "apps/infrastructure",
      "sourceRoot": "apps/infrastructure/src",
      "compilerOptions": {
        "tsConfigPath": "apps/infrastructure/tsconfig.app.json"
      }
    },
    "api": {
      "type": "application",
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    }
  }
}